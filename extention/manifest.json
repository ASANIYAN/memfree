{
  "manifest_version": 3,
  "name": "MemFree",
  "version": "1.0",
  "description": "Hybrid AI search and ask",
  "permissions": ["scripting", "bookmarks", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "css": ["index.css"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
